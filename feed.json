---
layout: none
permalink: /feed.json
---

{
  "posts": [
    {% for post in site.posts %}
    {
      "id": "{{ post.id | escape }}",
      "title": "{{ post.title | escape }}",
      "url": "{{ post.url | absolute_url }}",
      "date": "{{ post.date | date_to_xmlschema }}",
      "tags": "{{ post.tags | jsonify }}",
    }{% if forloop.last == false %},{% endif %}
    {% endfor %}
  ]
}
